<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- General Color Brushes -->
    
    <SolidColorBrush x:Key="BackgroundBrush" Color="#FF595959"/>
	<SolidColorBrush x:Key="TextBrush" Color="#FFF0F0F0"/>
	<SolidColorBrush x:Key="DarkBackgroundBrush" Color="#FF333333"/>
	<SolidColorBrush x:Key="BorderBrush" Color="#FFBDBDBD"/>
	<SolidColorBrush x:Key="ButtonTextBrush" Color="#FF1A1A1A"/>
	<SolidColorBrush x:Key="ButtonBorderBrush" Color="#FF1A1A1A"/>
	<LinearGradientBrush x:Key="ButtonBackgroundBrush" EndPoint="0,1" StartPoint="0,0">
		<GradientStop Color="#FFF3F3F3" Offset="0"/>
		<GradientStop Color="#FFCDCDCD" Offset="0.556"/>
		<GradientStop Color="#FF595959" Offset="1"/>
		<GradientStop Color="#FFCDCDCD" Offset="0.534"/>
	</LinearGradientBrush>
	<SolidColorBrush x:Key="ButtonForegroundSolidBrush" Color="#FFF0F0F0"/>
	<SolidColorBrush x:Key="ButtonBackgroundSolidBrush" Color="#FF333333"/>


</ResourceDictionary>